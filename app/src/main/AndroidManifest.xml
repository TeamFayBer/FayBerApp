<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="codepath.fayberapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/fayber"
        android:label="FayBerApp"
        android:roundIcon="@drawable/fayber"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="activities.FayActivity"
            android:label="FayBerApp"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="activities.LaunchActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="activities.AccueilActivity" />
        <activity android:name="activities.NosServicesActivity" />
        <activity android:name="activities.EquipeActivity" />
        <activity
            android:name="activities.DetailsActivity"
            android:label="@string/title_activity_detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="activities.SignInActivity"></activity>
    </application>

</manifest>